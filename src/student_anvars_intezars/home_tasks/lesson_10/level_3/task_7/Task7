interface Database {

    void save(Product product);

    Product findByTitle(String productTitle);

}


class Product {

    private String title;

    public Product(String title) {
        this.title = title;
    }

    public String getTitle() {
        return this.title;
    }

    Интефейс предоставленный выше не является функциоальным, так как
    содержит более чем 1 абстракный метод